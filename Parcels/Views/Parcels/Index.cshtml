<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Parcels</title>
    <link 
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" 
      rel="stylesheet" 
      integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" 
      crossorigin="anonymous">
  </head>
  <body>
    @using Parcels.Models;
    <h1>Estimate Parcels Shipping Price!</h1>
    @foreach (Parcel newParcel in Model)
    {
    
    <p>@newParcel.Length.ToString()</p>
    <p>@newParcel.Width.ToString()</p>
    <p>@newParcel.Height.ToString()</p>
    <p>@newParcel.Weight.ToString() lbs.</p>
    <p>@newParcel.CubicSize(newParcel.Length, newParcel.Width, newParcel.Height)</p>
    <p>$@newParcel.ShippingPrice(@newParcel.CubicSize(newParcel.Length, newParcel.Width, newParcel.Height), newParcel.Weight)</p>
    }

  </body>
</html>